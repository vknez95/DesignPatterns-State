<Window x:Class="StateUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
		<StackPanel HorizontalAlignment="Stretch" Name="stackPanel1" VerticalAlignment="Stretch" Width="Auto" Margin="0,50,0,0">
			<DataGrid AutoGenerateColumns="False" Height="100" Name="dataGrid1" Width="200" />
			
			<ListBox Height="100" Name="listBox1" Width="120" DataContext="{Binding}">
				<ItemsControl>
					<Label Content="Label" Height="28" Name="label1" />
					<Label Content="Label" Height="28" Name="label2" />
				</ItemsControl>
			</ListBox>
		</StackPanel>
		<Button Content="Button" Height="23" Name="button1" Width="75" Margin="12,12,416,276" />
		<Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="151,12,0,0" Name="button2" VerticalAlignment="Top" Width="75" />
	</Grid>
</Window>
